{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "dev:https": {
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    }
  },
  "globalEnv": [
    "NODE_ENV",
    "NEXT_PUBLIC_STAGING",
    "PORT",
    "ZUPOLL_SERVER_URL",
    "SEMAPHORE_HISTORIC_URL",
    "PASSPORT_SERVER",
    "ACCESS_TOKEN_SECRET",
    "BOT_TOKEN",
    "BOT_SUPERGROUP_ID",
    "BOT_CHANNEL_ID",
    "SITE_URL",
    "PCDPASS_USERS_GROUP_URL",
    "PCDPASS_HISTORIC_API_URL",
    "ZUZALU_PARTICIPANTS_GROUP_URL",
    "ZUZALU_ORGANIZERS_GROUP_URL",
    "IS_LOCAL_HTTPS",
    "NEXT_PUBLIC_IS_LOCAL_HTTPS",
    "BOT_ZUPOLL_LINK",
    "ALLOW_REPEAT_VOTES",
    "DEVCONNECT_ORGANIZERS_GROUP_URL",
    "DEVCONNECT_PARTICIPANTS_GROUP_URL"
  ]
}
