{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    }
  },
  "globalEnv": [
    "NODE_ENV",
    "NEXT_PUBLIC_STAGING",
    "PORT",
    "ZUPOLL_SERVER_URL",
    "SEMAPHORE_GROUP_URL",
    "SEMAPHORE_ADMIN_GROUP_URL",
    "SEMAPHORE_HISTORIC_URL",
    "PASSPORT_SERVER",
    "ACCESS_TOKEN_SECRET",
    "BOT_TOKEN",
    "BOT_SUPERGROUP_ID",
    "BOT_CHANNEL_ID",
    "SITE_URL"
  ]
}